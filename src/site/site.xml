<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 name="Planit DevOps"
         xmlns="http://maven.apache.org/DECORATION/1.6.0"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0
         http://maven.apache.org/xsd/decoration-1.6.0.xsd">
    <bannerLeft>
        <name>${project.name}</name>
        <href>${project.url}</href>
    </bannerLeft>
 
    <publishDate position="right" format="dd-MMMM-yyyy"/>
    <version/>
    <poweredBy>
    <logo name="Maven" href="http://maven.apache.org/"
          img="http://maven.apache.org/images/logos/maven-feather.png"/>
    </poweredBy>
 
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.5</version>
    </skin>
 
     <custom>
      <fluidoSkin>
        <topBarEnabled>true</topBarEnabled>
        <topBarContainerStyle>width: 90%;</topBarContainerStyle>
        <topBarIcon>
          <name>Planit Jira-Xray-Docker</name>
          <alt>Planit Jira-Xray-Docker</alt>
          <src>/images/planit.png</src>
          <href>/index.html</href>
        </topBarIcon>
        <sideBarEnabled>false</sideBarEnabled>
        <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      </fluidoSkin>
    </custom>
    <body>
        <menu ref="reports"/>
        <footer/>
    </body>
</project>